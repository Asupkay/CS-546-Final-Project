<div>
    <article id="items">
        {{#each items}}
            <button class="item">
                <p class="id">{{this.id}}</p>
                <p class="name">{{this.name}}</p>
            </button>
        {{/each}} 
    </article>
    <article id="orderLog">
        <ul id="orderList">
        </ul>
    </article>
    <button id="submitOrder">Submit</button>
</div>
